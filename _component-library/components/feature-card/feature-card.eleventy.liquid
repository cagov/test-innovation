<div class="c-feature-card wp-block-ca-design-system-hero cagov-with-sidebar cagov-with-sidebar-left cagov-featured-section cagov-bkgrd-gry cagov-block wp-block-cagov-hero">
  <div>
    <div class="cagov-stack cagov-p-2 cagov-featured-sidebar">
      <h1>{{ title }}</h1>
      <div class="cagov-hero-body-content">
        <p>
          {{ description }}
        </p>
        <div class="wp-block-button">
          <a class="btn-primary" href="{{ button.url }}">{{ button.text }}</a>
        </div>
      </div>
    </div>
    <div>
      <img
        class="cagov-featured-image"
        src="{{ image}}"
        alt="{{ image_alt }}"
        width="1024"
        height="683" />
    </div>
  </div>
</div>